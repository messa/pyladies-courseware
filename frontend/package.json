    "react": "^16.7.0",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.7.0",
    "react-relay": "^2.0.0",
    "semantic-ui-react": "^0.82.5",
    "subscriptions-transport-ws": "^0.9.15",
    "terser": "3.14.1"
